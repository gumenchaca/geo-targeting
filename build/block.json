{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/continent-block",
  "version": "0.1.0",
  "title": "Continent Block",
  "category": "widgets",
  "icon": "location",
  "description": "A block to select a continental region and highlight it with a border.",
  "supports": {
    "html": false,
    "align": [
      "wide",
      "full"
    ]
  },
  "textdomain": "continent-block",
  "attributes": {
    "continent": {
      "type": "string",
      "default": "none"
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}